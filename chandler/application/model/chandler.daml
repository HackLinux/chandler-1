<?xml version='1.0' encoding='ISO-8859-1'?>
<rdf:RDF
  xmlns:rdf      = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs     = "http://www.w3.org/2000/01/rdf-schema#"
  xmlns:daml     = "http://www.w3.org/2001/10/daml+oil"
  xmlns:xsd      = "http://www.w3.org/2000/10/XMLSchema"
  xmlns:ical     = "http://ilrt.org/discovery/2001/06/schemas/ical-full/hybrid.rdf#"
  xmlns:dc       = "http://purl.org/dc/elements/1.1/"
  xmlns:foaf     = "http://xmlns.com/foaf/0.1/"
  xmlns:chandler = "http://www.osafoundation.org/2002/chandler#"
  xmlns          = "http://www.osafoundation.org/2002/chandler#"
>

<daml:Ontology rdf:about"">
  <daml:versionInfo>$Id$</daml:versionInfo>
  <rdfs:comment>
    Chandler's PIM ontology
  </rdfs:comment>
</daml:Ontology>

<daml:Class rdf:ID="InformationItem">
  <rdfs:label>InformationItem</rdfs:label>
  <rdfs:comment>
    The basic unit of information in chandler.
  </rdfs:comment>
</daml:Class>

<daml:Class rdf:ID="EventItem">
  <rdfs:label>EventItem</rdfs:label>
  <rdfs:subclassOf rdf:resource="#InformationItem"/>
  <daml:sameClassAs rdf:resource="ical:VEVENT"/>
</daml:Class>

<daml:Class rdf:ID="TaskItem">
  <rdfs:label>TaskItem</rdfs:label>
  <rdfs:subclassOf rdf:resource="#InformationItem"/>
</daml:Class>

<daml:Class rdf:ID="PlaceItem">
  <rdfs:label>PlaceItem</rdfs:label>
  <rdfs:subclassOf rdf:resource="#InformationItem"/>
</daml:Class>

<daml:Class rdf:ID="NoteItem">
  <rdfs:label>NoteItem</rdfs:label>
  <rdfs:subclassOf rdf:resource="#InformationItem"/>
</daml:Class>

<daml:Class rdf:ID="DocumentItem">
  <rdfs:label>DocumentItem</rdfs:label>
  <rdfs:subclassOf rdf:resource="#InformationItem"/>
</daml:Class>

<daml:Class rdf:ID="BookmarkItem">
  <rdfs:label>BookmarkItem</rdfs:label>
