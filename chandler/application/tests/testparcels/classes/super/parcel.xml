<Parcel itemName="super"
        xmlns="http://osafoundation.org/parcels/core"
        xmlns:sub="http://testparcels.org/sub"
        xmlns:super="http://testparcels.org/super" >

  <namespace value="http://testparcels.org/super" />

  <Kind itemName="MySuperKind">
    <displayName>My Super Kind</displayName>
    <classes key="python">classes.super.Super.Super</classes>
    <attributes itemref="super:linksTo"/>
    <attributes itemref="super:linkedFrom"/>
  </Kind>

  <Attribute itemName="linksTo">
    <cardinality>single</cardinality>
    <inverseAttribute itemref="super:linkedFrom"/>
  </Attribute>

  <Attribute itemName="linkedFrom">
    <cardinality>single</cardinality>
    <inverseAttribute itemref="super:linksTo"/>
  </Attribute>

  <super:MySuperKind itemName="itemSuper">
    <linksTo itemref="sub:itemSub"/>
  </super:MySuperKind>

</Parcel>
