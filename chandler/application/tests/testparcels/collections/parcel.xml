<Parcel itemName="collections"
        xmlns="http://osafoundation.org/parcels/core"
        xmlns:coll="http://testparcels.org/collections" >

  <namespace value="http://testparcels.org/collections" />

  <Kind itemName="Block">
    <superKinds itemref="Item"/>

      <Attribute itemName="parentBlock">
         <type itemref="coll:Block"/>
         <cardinality>single</cardinality>
         <inverseAttribute itemref="coll:Block/childrenBlocks"/>
         <initialValue itemref="None"/>
       </Attribute>

       <Attribute itemName="childrenBlocks">
         <type itemref="coll:Block"/>
         <cardinality>list</cardinality>
         <inverseAttribute itemref="coll:Block/parentBlock"/>
         <initialValue/>
       </Attribute>

       <Attribute itemName="foo">
         <type itemref="coll:Block"/>
         <cardinality>dict</cardinality>
         <inverseAttribute itemref="coll:Block/oof"/>
         <initialValue/>
       </Attribute>

       <Attribute itemName="oof">
         <type itemref="coll:Block"/>
         <cardinality>dict</cardinality>
         <inverseAttribute itemref="coll:Block/foo"/>
         <initialValue/>
       </Attribute>
   </Kind>

</Parcel>
