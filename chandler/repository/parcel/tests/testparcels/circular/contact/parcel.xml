<Parcel itemName="contact"
        describes="//parcels/circular/contact"
        xmlns="//Schema/Core"
        xmlns:cal="//parcels/circular/calendar"
        xmlns:con="//parcels/circular/contact">


  <Attribute itemName="participatingIn">
    <displayName>Participant in Events</displayName>
    <cardinality>list</cardinality>
    <type itemref="cal:CalendarEvent"/>
    <inverseAttribute itemref="cal:participants"/>
  </Attribute>

  <Kind itemName="Contact">
    <displayName>Contact</displayName>
    <attributes itemref="con:participatingIn"/>
  </Kind>

  <con:Contact itemName="person1"/>

</Parcel>
