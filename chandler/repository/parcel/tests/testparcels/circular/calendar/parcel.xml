<Parcel itemName="calendar"
        describes="//parcels/circular/calendar"
        xmlns="//Schema/Core"
        xmlns:cal="//parcels/circular/calendar"
        xmlns:con="//parcels/circular/contact">

  <Attribute itemName="participants">
    <displayName>Participants</displayName>
    <cardinality>list</cardinality>
    <type itemref="con:Contact"/>
    <inverseAttribute itemref="con:participatingIn"/>
  </Attribute>

  <Kind itemName="CalendarEvent">
    <displayName>Calendar Event</displayName>
    <attributes itemref="cal:participants"/>
  </Kind>

  <cal:CalendarEvent itemName="event1">
    <cal:participants itemref="con:person1"/>
  </cal:CalendarEvent>

</Parcel>
