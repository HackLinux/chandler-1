<Parcel itemName="simple"
        xmlns="//Schema/Core"
        xmlns:simple="//parcels/simple" >

  <displayName>Simple Parcel</displayName>
  <description>Simple Parcel Loader Test Schema</description>
  <version>0.1</version>
  <author>Open Source Applications Foundation</author>

  <Kind itemName="TestKind">
    <displayName>Test Kind</displayName>
    <attributes itemref="simple:TestAttribute"/>
    <attributes itemref="simple:ListAttribute"/>
    <attributes itemref="simple:DictAttribute"/>
    <displayAttribute itemref="simple:TestAttribute"/>
  </Kind>

  <Kind itemName="SubKind">
    <superKinds itemref="simple:TestKind"/>
    <displayName>Subclass Test Kind</displayName>
  </Kind>

  <Attribute itemName="TestAttribute">
    <displayName>Test Attribute</displayName>
    <cardinality>single</cardinality>
    <type itemref="String"/>
    <initialValue type="String">XYZZY</initialValue>
  </Attribute>

  <Attribute itemName="ListAttribute">
    <displayName>List Attribute</displayName>
    <cardinality>list</cardinality>
    <type itemref="String"/>
    <initialValue/>
  </Attribute>

  <Attribute itemName="DictAttribute">
    <displayName>Dict Attribute</displayName>
    <cardinality>dict</cardinality>
    <type itemref="String"/>
    <initialValue/>
  </Attribute>

</Parcel>
