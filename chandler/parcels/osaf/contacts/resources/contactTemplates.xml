<?xml version="1.0"?>
<ContactTemplates>

<ContactTemplate type="Friend">
    <ContactMethod name="Main Email" />
    <ContactMethod name="Home Phone" />
    <ContactMethod name="Home Address" />
    <ContactMethod name="Jabber ID" />
    <Attribute position="header" name="http://www.osafoundation.org/2003/nickname"/>
    <Attribute position="header" name="http://www.osafoundation.org/2003/birthday"/>
    <Attribute position="body" name="http://www.osafoundation.org/2003/sharing"/>
    <Attribute position="body" name="http://www.osafoundation.org/2003/relationship"/>
