<?xml version="1.0"?>
<ContactTemplates>

<ContactTemplate type="Friend">
    <ContactMethod name="Main Email" />
    <ContactMethod name="Home Phone" />
    <ContactMethod name="Home Address" />
    <ContactMethod name="Jabber ID" />
    <Attribute position="header" name="nickname"/>
    <Attribute position="header" name="birthday"/>
    <Attribute position="body" name="sharing"/>
    <Attribute position="body" name="relationship"/>
    <Group name="Friends"/>
</ContactTemplate>

<ContactTemplate type="Company">
    <ContactMethod name="Main Email" />
    <ContactMethod name="Main Phone" />
    <ContactMethod name="Main Office Address" />
    <ContactClass type="Company"/>
    <Attribute position="body" name="sharing"/>
    <Group name="Companies"/>
</ContactTemplate>

<ContactTemplate type="Relative">
    <ContactMethod name="Main Email" />
    <ContactMethod name="Home Phone" />
    <ContactMethod name="Home Address" />
    <ContactMethod name="Jabber ID" />
    <Attribute position="header" name="gender"/>
    <Attribute position="header" name="birthday"/>
    <Attribute position="body" name="sharing"/>
    <Attribute position="body" name="relationship"/>
    <Group name="Family"/>
</ContactTemplate>

<ContactTemplate type="Coworker">
    <ContactMethod name="Work Phone" />
    <ContactMethod name="Jabber ID" />
    <Attribute position="header" name="jobTitle"/>
    <Attribute position="body" name="sharing"/>
    <Attribute position="body" name="relationship"/>
    <Group name="Coworkers"/>
</ContactTemplate>

<ContactTemplate type="Business Associate">
    <ContactMethod name="Main Email" />
    <ContactMethod name="Work Phone" />
    <ContactMethod name="Jabber ID" />
    <Attribute position="header" name="companyName"/>
    <Attribute position="header" name="jobTitle"/>
    <Attribute position="body" name="sharing"/>
    <Attribute position="body" name="relationship"/>
</ContactTemplate>

<ContactTemplate type="Professional">
    <ContactMethod name="Main Email" />
    <ContactMethod name="Work Phone" />
    <ContactMethod name="Website" />
    <ContactMethod name="Jabber ID" />
    <Attribute position="header" name="occupation"/>
    <Attribute position="body" name="sharing"/>
    <Attribute position="body" name="relationship"/>
</ContactTemplate>

</ContactTemplates>
