<?xml version="2.0" encoding="iso-8859-1"?>

<DomainSchema itemName="zaobao:RSSSchema"
	root="ZaoBao"
	xmlns="http://osafoundation.org/2003/08/schema_definition">
	
	<displayName>ZaoBao RSS Schema</displayName>
	<description>RSS schema for ZaoBao parcel for 0.2 release</description>
	<version>0.1</version>
	
	<itemPathMapping prefix="types"
		path="//Schema/Core/"/>
	<itemPathMapping prefix="zaobao"
		path="//Schema/RSSSchema/"/>
		
	<Attribute itemName="zaobao:creator">
		<displayName>Creator</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:description">
		<displayName>Description</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:language">
		<displayName>Language</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:link">
		<displayName>Web URL</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:title">
		<displayName>Title</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:category">
		<displayName>Category</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:encoding">
		<displayName>Encoding</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:etag">
		<displayName>eTag</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:lastModified">
		<displayName>Last Modified</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:DateTime"/>
	</Attribute>
	
	<Attribute itemName="zaobao:rssURL">
	        <displayName>RSS URL</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:String"/>
	</Attribute>
	
	<Attribute itemName="zaobao:isUnread">
	        <displayName>Is Unread</displayName>
		<cardinality>single</cardinality>
		<type itemref="types:Boolean"/>
	</Attribute>
	
	<Attribute itemName="zaobao:items">
	<displayName>Items</displayName>
	<cardinality>list</cardinality>
    	<type itemref="zaobao:RSSItem"/>
	<inverseAttribute itemref="zaobao:channel"/>
	</Attribute>

	<Attribute itemName="zaobao:channel">
	<displayName>Channel</displayName>
	<cardinality>single</cardinality>
    	<type itemref="zaobao:RSSChannel"/>
	<inverseAttribute itemref="zaobao:items"/>
	</Attribute>

	<Kind itemName="zaobao:RSSChannel">
		<displayName>RSS Channel</displayName>
		<attributes itemref="zaobao:creator"/>
		<attributes itemref="zaobao:description"/>
		<attributes itemref="zaobao:link"/>
		<attributes itemref="zaobao:title"/>
		<attributes itemref="zaobao:category"/>
		<attributes itemref="zaobao:language"/>
		<attributes itemref="zaobao:encoding"/>
		<attributes itemref="zaobao:etag"/>
		<attributes itemref="zaobao:lastModified"/>
		<attributes itemref="zaobao:rssURL"/>
		<attributes itemref="zaobao:isUnread"/>
		<attributes itemref="zaobao:items"/>
		<classes key="python">OSAF.zaobao.model.RSSData.RSSChannel</classes>
	</Kind>	

	<Kind itemName="zaobao:RSSItem">
		<displayName>RSS Item</displayName>
		<attributes itemref="zaobao:creator"/>
		<attributes itemref="zaobao:description"/>
		<attributes itemref="zaobao:link"/>
		<attributes itemref="zaobao:title"/>
		<attributes itemref="zaobao:category"/>
		<attributes itemref="zaobao:lastModified"/>
		<attributes itemref="zaobao:channel"/>
		<classes key="python">OSAF.zaobao.model.RSSData.RSSItem</classes>
	</Kind>
	
<!-- end -->	
</DomainSchema>
			
