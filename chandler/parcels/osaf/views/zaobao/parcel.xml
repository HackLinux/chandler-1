             xmlns:events="//parcels/osaf/framework/blocks/Events"
             xmlns:zb="//parcels/osaf/examples/zaobao">

  <core:version>0.3</core:version>

  <!-- RSS Channels -->
  <zb:RSSChannel itemName="osaf">
    <displayName>Loading... OSAF News</displayName>
    <zb:url>http://www.osafoundation.org/rss/2.0/</zb:url>
  </zb:RSSChannel>
  <zb:RSSChannel itemName="chandlerwiki">
    <displayName>Loading... Chandler Wiki</displayName>
    <zb:url>http://wiki.osafoundation.org/twiki/bin/view/Chandler/WebRss?skin=rss</zb:url>
  </zb:RSSChannel>
  <zb:RSSChannel itemName="junglewiki">
    <displayName>Loading... Jungle Wiki</displayName>
    <zb:url>http://wiki.osafoundation.org/twiki/bin/view/Jungle/WebRss?skin=rss</zb:url>
  </zb:RSSChannel>
  <zb:RSSChannel itemName="ted">
    <displayName>Loading... Ted Leung on the air</displayName>
    <zb:url>http://www.sauria.com/blog?flav=rss</zb:url>
  </zb:RSSChannel>
  <zb:RSSChannel itemName="zaobao">
    <displayName>Loading... ZaoBao news</displayName>
    <zb:url>http://blogs.osafoundation.org/zaobao/index.rdf</zb:url>
  </zb:RSSChannel>
  <zb:RSSChannel itemName="chao">
    <displayName>Loading... Chao's Blog</displayName>
    <zb:url>http://blogs.osafoundation.org/chao/index.rdf</zb:url>
  </zb:RSSChannel>
  <zb:RSSChannel itemName="lessig">
    <displayName>Loading... Lessig Blog</displayName>
    <zb:url>http://lessig.org/blog/index.xml</zb:url>
  </zb:RSSChannel>
  <zb:RSSChannel itemName="diveintomark">
    <displayName>Loading... dive in to mark</displayName>
    <zb:url>http://diveintomark.org/xml/rss.xml</zb:url>
  </zb:RSSChannel>


  <!-- Styles -->
  <CharacterStyle itemName="HeaderCharacterStyle">
    <fontStyle>bold</fontStyle>
  </CharacterStyle>  

  <!-- The main ZaoBao view -->

  <Tree itemName="ZaoBaoTree">
    <elementDelegate>osaf.examples.zaobao.blocks.ZaoBaoTreeDelegate</elementDelegate>
    <rootPath itemref="zb:RSSChannel"/>

    <columnHeadings>Subject</columnHeadings>
    <columnWidths>300</columnWidths>
    <columnHeadings>Date</columnHeadings>
    <columnWidths>150</columnWidths>

    <size>600,200</size>
    <minimumSize>400,100</minimumSize>
  </Tree>

  <zb:ZaoBaoItemDetail itemName="ItemDetail">
    <size>100,50</size>
    <minimumSize>100,50</minimumSize>
  </zb:ZaoBaoItemDetail>

  <EditText itemName="NewChannel">
    <size>100,20</size>
    <minimumSize>100,20</minimumSize>
    <border>3.0, 3.0, 3.0, 3.0</border>
    <stretchFactor>0.0</stretchFactor>
    <lineStyleEnum>SingleLine</lineStyleEnum>
    <textStyleEnum>PlainText</textStyleEnum>
    <readOnly>False</readOnly>
    <characterStyle itemref="doc:HeaderCharacterStyle"/>
  </EditText>

  <BoxContainer itemName="ItemViewBox">
    <size>200,100</size>
    <minimumSize>200,100</minimumSize>
    <border>2.0, 2.0, 2.0, 2.0</border>
    <orientationEnum>Vertical</orientationEnum>

    <childrenBlocks itemref="doc:ItemDetail"/>
    <childrenBlocks itemref="doc:NewChannel"/>

  </BoxContainer>

  <HTML itemName="AboutZaoBao">
    <url>parcels/osaf/examples/zaobao/AboutZaoBao.html</url>
    <eventBoundary>True</eventBoundary>
    <size>900,580</size>
    <minimumSize>900,580</minimumSize>
    <border>1.0, 1.0, 1.0, 1.0</border>
  </HTML>

  <SplitterWindow itemName="ZaoBaoView">
    <eventBoundary>True</eventBoundary>
    <size>900,580</size>
    <minimumSize>900,580</minimumSize>
    <border>2.0, 2.0, 2.0, 2.0</border>

    <childrenBlocks itemref="doc:ZaoBaoTree"/>
    <childrenBlocks itemref="doc:ItemViewBox"/>
  </SplitterWindow>

  <!-- Example single channel view -->

  <Tree itemName="ChannelList">
    <elementDelegate>osaf.examples.zaobao.blocks.ZaoBaoListDelegate</elementDelegate>
    <rootPath itemref="doc:osaf"/>

    <columnHeadings>Subject</columnHeadings>
    <columnWidths>300</columnWidths>
    <columnHeadings>Date</columnHeadings>
