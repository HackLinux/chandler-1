<?xml version="1.0" encoding="iso-8859-1"?>

<Parcel describes="//parcels/osaf/framework/webdav"
        itsName="webdav"
        xmlns="http://osafoundation.org/parcels/core"
        xmlns:dav="http://osafoundation.org/parcels/osaf/framework/webdav">

  <Attribute itsName="baseURL">
    <type itemref="URL"/>
  </Attribute>

  <Attribute itsName="username">
    <type itemref="String"/>
  </Attribute>

  <Attribute itsName="password">
    <type itemref="String"/>
  </Attribute>

  <Kind itsName="WebDAVServer">
    <superKinds itemref="Item"/>
    <attributes itemref="dav:baseURL"/>
    <attributes itemref="dav:username"/>
    <attributes itemref="dav:password"/>
  </Kind>

</Parcel>
