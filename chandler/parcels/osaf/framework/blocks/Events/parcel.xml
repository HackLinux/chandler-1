<core:Parcel itsName="Events"
             xmlns:core="http://osafoundation.org/parcels/core"
             xmlns="http://osafoundation.org/parcels/osaf/framework/blocks"
             xmlns:events="http://osafoundation.org/parcels/osaf/framework/blocks/Events">

  <description>Events need to be defined in a different parcel than the schema because of a
limitation of the XML format. Applicaiton specific events should not be located here. 
Instead put them in a View in your application. Also don't put any references to 
application specific parcels here</description>
  <version>0.3</version>

  <!-- Block Events dispatched by name -->
  <EventList itsName="GlobalEvents">
    <!-- Don't add any events here unless your proposed event has been reviewed -->
    <eventsForNamedDispatch itemref="events:About"/>    
    <eventsForNamedDispatch itemref="events:Close"/>
    <eventsForNamedDispatch itemref="events:PrintPreview"/>
    <eventsForNamedDispatch itemref="events:Print"/>
    <eventsForNamedDispatch itemref="events:Copy"/>
    <eventsForNamedDispatch itemref="events:Cut"/>
    <eventsForNamedDispatch itemref="events:Delete"/>
    <eventsForNamedDispatch itemref="events:New"/>
    <eventsForNamedDispatch itemref="events:Open"/>
    <eventsForNamedDispatch itemref="events:Paste"/>
    <eventsForNamedDispatch itemref="events:Preferences"/>
    <eventsForNamedDispatch itemref="events:Quit"/>
    <eventsForNamedDispatch itemref="events:Redo"/>
    <eventsForNamedDispatch itemref="events:SelectedDateChanged"/>
    <eventsForNamedDispatch itemref="events:SelectItemBroadcast"/>
    <eventsForNamedDispatch itemref="events:SelectItemBroadcastInsideActiveView"/>
    <eventsForNamedDispatch itemref="events:SetContents"/>
    <eventsForNamedDispatch itemref="events:Undo"/>

    <eventsForNamedDispatch itemref="events:EnterPressed"/>
  </EventList>

  <BlockEvent itsName="About">
    <blockName>About</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Close">
    <blockName>Close</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="PrintPreview">
    <blockName>PrintPreview</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Print">
    <blockName>Print</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Copy">
    <blockName>Copy</blockName>
    <dispatchEnum>FocusBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Cut">
    <blockName>Cut</blockName>
    <dispatchEnum>FocusBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Delete">
    <blockName>Delete</blockName>
    <dispatchEnum>FocusBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="New">
    <blockName>New</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Open">
    <blockName>Open</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Paste">
    <blockName>Paste</blockName>
    <dispatchEnum>FocusBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Preferences">
    <blockName>Preferences</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Quit">
    <blockName>Quit</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Redo">
    <blockName>Redo</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="SelectedDateChanged">
    <blockName>SelectedDateChanged</blockName>
    <dispatchEnum>BroadcastEverywhere</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="SelectItemBroadcast">
    <blockName>SelectItemBroadcast</blockName>
    <methodName>onSelectItemEvent</methodName>
    <dispatchEnum>BroadcastInsideMyEventBoundary</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="SelectItemBroadcastInsideActiveView">
    <blockName>SelectItemBroadcastInsideActiveView</blockName>
    <methodName>onSelectItemEvent</methodName>
    <dispatchEnum>BroadcastInsideActiveViewEventBoundary</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="SetContents">
    <blockName>SetContents</blockName>
    <dispatchEnum>BroadcastInsideMyEventBoundary</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="Undo">
    <blockName>Undo</blockName>
    <dispatchEnum>ActiveViewBubbleUp</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="EnterPressed">
    <blockName>EnterPressed</blockName>
    <dispatchEnum>BroadcastInsideMyEventBoundary</dispatchEnum>
  </BlockEvent>

  <BlockEvent itsName="SendShareItem">
    <blockName>SendShareItem</blockName>
    <dispatchEnum>FocusBubbleUp</dispatchEnum>
  </BlockEvent>
</core:Parcel>
