  </blocks:StaticText>

  <blocks:PresentationStyle itsName="HeadlinePresentation">
    <sampleText></sampleText><!-- empty sample means "use displayname" -->
    <editInPlace>True</editInPlace>
  </blocks:PresentationStyle>

