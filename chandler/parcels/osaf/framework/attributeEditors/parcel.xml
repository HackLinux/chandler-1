<Parcel itsName="attributeEditors"
        xmlns="http://osafoundation.org/parcels/core"
        xmlns:contentModel="http://osafoundation.org/parcels/osaf/contentmodel"
        xmlns:doc="http://osafoundation.org/parcels/osaf/framework/attributeEditors">

  <version>0.3</version>

  <Kind itsName="AttributeEditor">
    <superKinds itemref="Item"/>
    
    <Attribute itsName="className">
      <type itemref="String"/>
    </Attribute>
  </Kind>

  <contentModel:ItemCollection itsName="AttributeEditors">
    <_rule value="for i inevery '//parcels/osaf/framework/attributeEditors/AttributeEditor' where True"/>
  </contentModel:ItemCollection>

  <doc:AttributeEditor itsName="_default"><className>osaf.framework.attributeEditors.AttributeEditors.RepositoryAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="Boolean"><className>osaf.framework.attributeEditors.AttributeEditors.CheckboxAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="Contact"><className>osaf.framework.attributeEditors.AttributeEditors.ContactAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="ContactName"><className>osaf.framework.attributeEditors.AttributeEditors.ContactNameAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="ContentItem"><className>osaf.framework.attributeEditors.AttributeEditors.StringAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="DateTime"><className>osaf.framework.attributeEditors.AttributeEditors.DateTimeAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="TimeDelta"><className>osaf.framework.attributeEditors.AttributeEditors.TimeDeltaAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="TimeDelta+reminderPopup"><className>osaf.framework.attributeEditors.AttributeEditors.ReminderDeltaAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="NoneType+reminderPopup"><className>osaf.framework.attributeEditors.AttributeEditors.ReminderDeltaAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="EmailAddress"><className>osaf.framework.attributeEditors.AttributeEditors.EmailAddressAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="Kind"><className>osaf.framework.attributeEditors.AttributeEditors.StampAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="Location"><className>osaf.framework.attributeEditors.AttributeEditors.LocationAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="SharingStatusEnum"><className>osaf.framework.attributeEditors.AttributeEditors.EnumAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="String"><className>osaf.framework.attributeEditors.AttributeEditors.StringAttributeEditor</className></doc:AttributeEditor>
  <!-- XXX This Integer is a hack, it should work without since default should map to whatever is defined in repository -->
  <doc:AttributeEditor itsName="Integer"><className>osaf.framework.attributeEditors.AttributeEditors.StringAttributeEditor</className></doc:AttributeEditor>
  <doc:AttributeEditor itsName="TimeTransparencyEnum"><className>osaf.framework.attributeEditors.AttributeEditors.ChoiceAttributeEditor</className></doc:AttributeEditor>

</Parcel>
